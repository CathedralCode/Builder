{
	"name": "cathedral/builder",
	"description": "Database Layer builder library with Basic Web & Console interface",
	"type": "library",
	"keywords": [
		"inane",
		"cathedral",
        "laminas",
		"db"
	],
	"homepage": "https://git.inane.co.za:3000/Cathedral/builder",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packer.inane.co.za",
            "name": "Inane Packer"
        }
    ],
	"authors": [
		{
			"name": "Philip Michael Raab",
			"email": "peep@inane.co.za",
			"homepage": "https://inane.co.za",
			"role": "Developer"
		}
	],
	"support": {
		"email": "peep@inane.co.za",
		"wiki": "https://git.inane.co.za:3000/Cathedral/builder/wiki",
		"issues": "https://git.inane.co.za:3000/Cathedral/builder/issues"
	},
	"require": {
		"php": ">=7.2",
		"laminas/laminas-paginator": ">=2.8",
		"laminas/laminas-db": ">2.11",
        "laminas/laminas-hydrator": "^1.1 || ^2.1 || ^3.0 || ^4.1",
        "laminas/laminas-mvc-console": ">=1.2"
	},
	"autoload": {
		"psr-4": {
			"Cathedral\\Builder\\": "src/"
		}
	},
    "extra": {
        "laminas": {
            "module": [
                "Cathedral"
            ]
        }
    },
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true
	}
}